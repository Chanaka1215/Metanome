<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">

<module rename-to="hello">
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="de.uni_potsdam.hpi.metanome.Metanome_Dependencies"/>
	
	<entry-point class="de.uni_potsdam.hpi.metanome.frontend.client.MetanomeEntryPoint"/>

	<source path="client"/>
	
	<servlet path="/parameterService"
			 class="de.uni_potsdam.hpi.metanome.frontend.server.ParameterServiceImpl" />
	<servlet path="/finderService"
			 class="de.uni_potsdam.hpi.metanome.frontend.server.FinderServiceImpl" />
	<servlet path="/executionService"
			 class="de.uni_potsdam.hpi.metanome.frontend.server.ExecutionServiceImpl" />
	<servlet path="/inputDataService"
			 class="de.uni_potsdam.hpi.metanome.frontend.server.InputDataServiceImpl" />
	
    <!-- Changing the linker is only necessary if you want to try out SuperDevMode -->
    <add-linker name="xsiframe"/>
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>
    
    <!-- Inherit the default GWT style sheet. You can change       -->
	<!-- the theme of your GWT application by uncommenting          -->
	<!-- any one of the following lines.                           -->
	<inherits name='com.google.gwt.user.theme.standard.Standard'/>
	<!-- <inherits name="com.google.gwt.user.theme.chrome.Chrome"/> -->
	<!-- <inherits name="com.google.gwt.user.theme.dark.Dark"/> -->
</module>

