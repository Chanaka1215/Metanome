<?xml version="1.0" encoding="UTF-8"?>
<web-app>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Hello.html</welcome-file>
  </welcome-file-list>
  
  <!-- Servlets -->
  <servlet>
  	<servlet-name>parameterServiceImpl</servlet-name>
  	<servlet-class>de.uni_potsdam.hpi.metanome.frontend.server.ParameterServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>parameterServiceImpl</servlet-name>
  	<url-pattern>/hello/parameterService</url-pattern>
  </servlet-mapping>
  
  <servlet> 
  	<servlet-name>finderServiceImpl</servlet-name> 
  	<servlet-class>de.uni_potsdam.hpi.metanome.frontend.server.FinderServiceImpl</servlet-class> 
  </servlet> 
  <servlet-mapping> 
  	<servlet-name>finderServiceImpl</servlet-name> 
  	<url-pattern>/hello/finderService</url-pattern> 
  </servlet-mapping>
  
  <servlet> 
  	<servlet-name>executionServiceImpl</servlet-name> 
  	<servlet-class>de.uni_potsdam.hpi.metanome.frontend.server.ExecutionServiceImpl</servlet-class> 
  </servlet> 
  <servlet-mapping> 
  	<servlet-name>executionServiceImpl</servlet-name> 
  	<url-pattern>/hello/executionService</url-pattern> 
  </servlet-mapping>

</web-app>